		var currentTabId ='V';
		function enableTab (id)	{
			if (id == currentTabId) {
				return;
			}
			obj = document.getElementById ('panel' + id);
			obj.className = "visiblePanel";
			obj = document.getElementById ('tab' + id);
			obj.className = "visibleTab";
			obj = document.getElementById ('panel' + currentTabId);
			obj.className = "hiddenPanel";
			obj = document.getElementById ('tab' + currentTabId);
			obj.className = "hiddenTab";
			currentTabId = id;
		}
</script>
