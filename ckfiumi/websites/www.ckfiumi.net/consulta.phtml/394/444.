function initialize() {
	 var locations = [
      ['Imbarco/Put In', 46.250277777778, 12.4625, 1],
      ['Sbarco/Take Off', 46.2275, 12.486388888889, 2]
    ];
	var myOptions = {
		center: new google.maps.LatLng((46.250277777778+46.2275)/2, (12.4625+12.486388888889)/2),
      zoom: 12,
      mapTypeId: google.maps.MapTypeId.HYBRID
   };
   var map = new google.maps.Map(document.getElementById("map"),myOptions);

	var infowindow = new google.maps.InfoWindow();
   var marker, i;

   for (i = 0; i < locations.length; i++) {  
      marker = new google.maps.Marker({
        position: new google.maps.LatLng(locations[i][1], locations[i][2]),
        map: map
      });
      google.maps.event.addListener(marker, 'click', (function(marker, i) {
        return function() {
          infowindow.setContent(locations[i][0]);
          infowindow.open(map, marker);
        }
      })(marker, i));
   }
}
function loadScript() {
  var script = document.createElement("script");
  script.type = "text/javascript";
  script.src = "http://maps.googleapis.com/maps/api/js?key=AIzaSyCwovf7SW04dMtSIftHEgxPTRkYI42Ejyg&sensor=false&callback=initialize";
  document.body.appendChild(script);
}
window.onload = loadScript;
</script>
